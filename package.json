{
  "name": "autogitgrow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:backend": "python -m uvicorn backend.main:app --reload",
    "start:frontend": "cd frontend && npm run dev",
    "start:autostarback": "python -u scripts/autostarback.py",
    "start:autostargrow": "python -u scripts/autostargrow.py",
    "start:autotrack": "python -u scripts/autotrack.py",
    "start:autounstarback": "python -u scripts/autounstarback.py",
    "start:generate_batch_size": "python -u scripts/generate_batch_size.py",
    "start:gitgrow": "python -u scripts/gitgrow.py",
    "start:maintainer": "python -u scripts/maintainer.py",
    "start:shoutouts": "python -u scripts/shoutouts.py",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down"
  },
  "dependencies": {
    "concurrently": "^9.2.1"
  },
  "devDependencies": {}
}
